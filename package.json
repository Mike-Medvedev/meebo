{
  "name": "meebo",
  "version": "0.4.2",
  "description": "the missing api contract validation for express js",
  "license": "ISC",
  "author": "Michael Medvedev",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Mike-Medvedev/meebo"
  },
  "scripts": {
    "build": "tsup package/index.ts --format esm --dts --out-dir dist --external zod --external express --external swagger-ui-express --external @asteasolutions/zod-to-openapi && sed 's|./shared.ts|./index.js|g' package/types.d.ts > dist/types.d.ts && echo '/// <reference path=\"./types.d.ts\" />' | cat - dist/index.d.ts > temp && mv temp dist/index.d.ts",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "express": "^5.0.0",
    "zod": "^3.0.0 || ^4.0.0"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.2.0",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@vitest/coverage-v8": "^4.0.15",
    "express": "^5.2.1",
    "supertest": "^7.1.4",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15",
    "zod": "^4.1.13"
  }
}
